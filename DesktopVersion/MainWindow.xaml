<Window x:Class="DesktopVersion.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DesktopVersion"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button Name="LogIn" Click="LogIn_OnClick" Margin="0 0 0 40" Width="100" Height="30" HorizontalAlignment="Center" VerticalAlignment="Center" Content="Log in"/>
        <Button Name="SignUp" Click="SignUp_OnClick" Margin="0 40 0 0" Width="100" Height="30" HorizontalAlignment="Center" VerticalAlignment="Center" Content="Sign up"/>
    </Grid>
</Window>
